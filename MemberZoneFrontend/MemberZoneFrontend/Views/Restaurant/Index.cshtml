
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div class="jumbotron">
    <h1>ASP>NET</h1>
    @DateTime.Now.Year
    <div>
        There are @Enumerable.Count(Model) number of restaurants in the db.
    </div>
</div>

<ul>
    @foreach (var restaurant in Model)
    {
        <li>@restaurant.Name</li>
    }
</ul>

